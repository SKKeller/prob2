<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
          <menuContribution
            locationURI="menu:debug">
         <command
               commandId="de.prob.ui.groovyconsole"
               label="Groovy Console"
               mnemonic="G"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
              <command
            categoryId="de.prob.ui.commands.category"
            id="de.prob.ui.groovyconsole"
            name="Open Groovy Console">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
 <handler
            commandId="de.prob.ui.groovyconsole">
         <class
               class="de.prob.ui.OpenGroovyConsole">
         </class>
      </handler>
   </extension>

</plugin>
