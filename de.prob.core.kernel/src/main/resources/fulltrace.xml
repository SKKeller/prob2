<included>

<appender  name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
	<encoder>
		<pattern>%d %-4relative [%thread] %-5level %logger{35} Method: %method Line: %line - %msg%n%ex{full}</pattern>
	</encoder>
</appender>

<logger name="de.prob" level="ERROR" />
<logger name="de.prob.worksheet" level="TRACE" />
<logger name="de.prob.ui.worksheet" level="TRACE" />

<root level="ERROR">
	<appender-ref ref="STDOUT" />
</root>
</included>